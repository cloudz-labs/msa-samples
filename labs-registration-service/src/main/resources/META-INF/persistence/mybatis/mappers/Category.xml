<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="category">
	
	<select id="category.selectCategoryList" resultType="io.dtlabs.registration.category.vo.Category">
		SELECT * FROM Categories	
	</select>
	
	<select id="category.selectCategoryById" parameterType="java.lang.Integer" resultType="io.dtlabs.registration.category.vo.Category">
		SELECT * FROM Categories WHERE id=#{id}
	</select>
	
</mapper>
